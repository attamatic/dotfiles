{
    "list.source.files.excludePatterns": ["**/.git/**", "**/node_modules/**"],
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "suggest.maxPreviewWidth": 80,
    "suggest.enablePreview": true,
    "suggest.labelMaxLength": 70,
    "suggest.detailMaxLength": 100,
    "suggest.snippetIndicator": "~",
    "suggest.minTriggerInputLength": 1,
    "suggest.triggerCompletionWait": 100,
    "suggest.removeDuplicateItems": false,
    "suggest.defaultSortMethod": "length",
    "suggest.completionItemKindLabels": {
        "text": "[Text]",
        "method": "[Method]",
        "function": "[Function]",
        "constructor": "[Constructor]",
        "field": "[Field]",
        "variable": "[Variable]",
        "class": "[Class]",
        "interface": "[Interface]",
        "module": "[Module]",
        "property": "[Property]",
        "unit": "[Unit]",
        "value": "[Value]",
        "enum": "[Enum]",
        "keyword": "[Keyword]",
        "snippet": "[Snippet]",
        "color": "[Color]",
        "file": "[File]",
        "reference": "[Reference]",
        "folder": "[Folder]",
        "enumMember": "[EnumMember]",
        "constant": "[Constant]",
        "struct": "[Struct]",
        "event": "[Event]",
        "operator": "[Operator]",
        "typeParameter": "[TypeParam]",
        "default": "[Default]"
    },
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": ">>",
    "diagnostic.warningSign": ">>",
    "diagnostic.infoSign": ">>",
    "diagnostic.hintSign": ">>",
    "diagnostic.maxWindowHeight": 8,
    "diagnostic.maxWindowWidth": 80,
    "diagnostic.format": "%message -%code [%severity] [%source]",
    "diagnostic.showUnused": true,
    "diagnostic.showDeprecated": true,
    "signature.triggerSignatureWait": 500,
    "signature.target": "float",
    "signature.maxWindowWidth": 80,
    "signature.maxWindowHeight": 8,
    "signature.preferShownAbove": false,
    "signature.hideOnTextChange": false,
    "codeLens.enable": false,
    "hover.target": "float",
    "hover.previewMaxHeight": 12,
    "hover.floatMaxWidth": 80,
    "hover.floatMaxHeight": 20,
    "dialog.maxWidth": 80,
    "dialog.maxHeight": 20,
    "dialog.confirmKey": "<cr>",
    "dialog.pickerButtons": true,
    "dialog.pickerButtonShortcut": true,
    "dialog.floatHighlight": null,
    "dialog.floatBorderHighlight": null,
    "notification.marginTop": 1,
    "notification.marginRight": 1,
    "notification.maxWidth": 60,
    "notification.maxHeight": 10,
    "notification.highlightGroup": "CocFloating",
    "notification.minProgressWidth": 30,
    "workspace.ignoredFiletypes": ["markdown", "log", "txt", "help"],
    "list.indicator": ">",
    "list.alignColumns": false,
    "list.height": 10,
    "list.selectedSignText": "*",
    "list.maxPreviewHeight": 12,
    "list.previewSplitRight": false,
    "list.matchHighlightGroup": "Search",
    "list.previewHighlightGroup": "Search",
    "coc.preferences.formatOnSaveFiletypes": [],
    "coc.preferences.formatOnType": false,
    "coc.preferences.formatOnTypeFiletypes": [],
    "coc.preferences.excludeImageLinksInMarkdownDocument": true,
    "coc.source.file.ignoreHidden": true,
    "Lua.telemetry.enable": false,
    "solargraph.diagnostics": true
}
